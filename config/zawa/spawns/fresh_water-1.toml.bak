
#
# This is 1 of ZAWA's 10 habitat ranges. Each sub-range has its own list of biomes, and all of the sub-range's entities will spawn in those biomes.
# Disable entities (or a species variant) by setting "enabled" under their category to false.
# Use ONLY whole numbers greater than 0 for chance, min, and max!
# "chance" is how common they spawn, a higher number is more common than a lower number. "min" and "max" adjusts their spawning group size.
# !! If modded biomes are not generated, delete the entire "biomes" line for each sub-category and restart minecraft. !!
#
#
[fast_fresh_water]
	biomes = ["minecraft:river", "minecraft:frozen_river", "demon_realm:boiling_river", "atum:dried_river", "terraforged:frozen_lake", "terraforged:lake", "twilightforest:stream"]

	[fast_fresh_water.salmon]
		chance = 10
		min = 6
		max = 6
		enabled = true

[slow_fresh_water]
	biomes = ["minecraft:swamp", "minecraft:swamp_hills", "biomesoplenty:bayou_mangrove", "biomesoplenty:bayou", "biomesoplenty:deep_bayou", "biomesoplenty:dense_marsh", "biomesoplenty:highland_moor", "biomesoplenty:marsh", "biomesoplenty:muskeg", "biomesoplenty:ominous_mire", "biomesoplenty:rainforest_floodplain", "biomesoplenty:shroomy_wetland", "biomesoplenty:tundra_bog", "biomesoplenty:wetland", "biomesoplenty:wetland_forest", "projectvibrantjourneys:mangrove_marsh", "projectvibrantjourneys:willow_wetlands", "villagers_and_mosnetrs_legacy:swamp_cave", "terraforged:marshland", "twilightforest:swamp", "twilightforest:fire_swamp", "terrestria:cypress_swamp", "petfish:suba", "projectvibrantjourneys:mangrove_marsh", "projectvibrantjourneys:willow_wetlands", "terrestria:caldera", "terrestria:oasis", "terrestria:rainbow_rainforest_lake"]

	[slow_fresh_water.flamingo]
		chance = 15
		min = 4
		max = 6
		enabled = true

